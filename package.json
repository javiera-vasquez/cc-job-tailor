{
  "version": "0.5.0",
  "dependencies": {
    "@react-pdf/renderer": "^4.3.0",
    "@types/js-yaml": "^4.0.9",
    "js-yaml": "^4.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "zod": "^3.23.8"
  },
  "name": "cc-resume-manager",
  "type": "module",
  "private": false,
  "license": "CC-BY-NC-4.0",
  "scripts": {
    "dev": "bun dev-with-watch.ts",
    "save-to-pdf": "bun scripts/generate-data.ts \"$@\" && bun scripts/generate-pdf.ts \"$@\"",
    "generate-data": "bun scripts/generate-data.ts",
    "generate-pdf": "bun scripts/generate-pdf.ts",
    "validate:metadata": "bun scripts/validate-metadata.ts",
    "validate:resume": "bun scripts/validate-resume.ts",
    "validate:job-analysis": "bun scripts/validate-job-analysis.ts",
    "validate:cover-letter": "bun scripts/validate-cover-letter.ts",
    "test": "bun test test/",
    "test:watch": "bun test test/ --watch",
    "test:coverage": "bun test test/ --coverage",
    "tsc": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "prettier": "^3.6.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
